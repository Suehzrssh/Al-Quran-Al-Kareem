{
  "number": 82,
  "name": "al-Infitar",
  "verses": [
    {
      "text": "إِذَا ٱلسَّمَآءُ ٱنفَطَرَتْ",
      "numberInSurah": 1,
      "juz": 30,
      "page": 587,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5830.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَإِذَا ٱلْكَوَاكِبُ ٱنتَثَرَتْ",
      "numberInSurah": 2,
      "juz": 30,
      "page": 587,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5831.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَإِذَا ٱلْبِحَارُ فُجِّرَتْ",
      "numberInSurah": 3,
      "juz": 30,
      "page": 587,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5832.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَإِذَا ٱلْقُبُورُ بُعْثِرَتْ",
      "numberInSurah": 4,
      "juz": 30,
      "page": 587,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5833.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "عَلِمَتْ نَفْسٌ مَّا قَدَّمَتْ وَأَخَّرَتْ",
      "numberInSurah": 5,
      "juz": 30,
      "page": 587,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5834.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "يَٰٓأَيُّهَا ٱلْإِنسَٰنُ مَا غَرَّكَ بِرَبِّكَ ٱلْكَرِيمِ",
      "numberInSurah": 6,
      "juz": 30,
      "page": 587,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5835.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "ٱلَّذِى خَلَقَكَ فَسَوَّىٰكَ فَعَدَلَكَ",
      "numberInSurah": 7,
      "juz": 30,
      "page": 587,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5836.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "فِىٓ أَىِّ صُورَةٍ مَّا شَآءَ رَكَّبَكَ",
      "numberInSurah": 8,
      "juz": 30,
      "page": 587,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5837.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "كَلَّا بَلْ تُكَذِّبُونَ بِٱلدِّينِ",
      "numberInSurah": 9,
      "juz": 30,
      "page": 587,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5838.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَإِنَّ عَلَيْكُمْ لَحَٰفِظِينَ",
      "numberInSurah": 10,
      "juz": 30,
      "page": 587,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5839.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "كِرَامًا كَٰتِبِينَ",
      "numberInSurah": 11,
      "juz": 30,
      "page": 587,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5840.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "يَعْلَمُونَ مَا تَفْعَلُونَ",
      "numberInSurah": 12,
      "juz": 30,
      "page": 587,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5841.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "إِنَّ ٱلْأَبْرَارَ لَفِى نَعِيمٍ",
      "numberInSurah": 13,
      "juz": 30,
      "page": 587,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5842.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَإِنَّ ٱلْفُجَّارَ لَفِى جَحِيمٍ",
      "numberInSurah": 14,
      "juz": 30,
      "page": 587,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5843.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "يَصْلَوْنَهَا يَوْمَ ٱلدِّينِ",
      "numberInSurah": 15,
      "juz": 30,
      "page": 587,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5844.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَمَا هُمْ عَنْهَا بِغَآئِبِينَ",
      "numberInSurah": 16,
      "juz": 30,
      "page": 587,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5845.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَمَآ أَدْرَىٰكَ مَا يَوْمُ ٱلدِّينِ",
      "numberInSurah": 17,
      "juz": 30,
      "page": 587,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5846.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "ثُمَّ مَآ أَدْرَىٰكَ مَا يَوْمُ ٱلدِّينِ",
      "numberInSurah": 18,
      "juz": 30,
      "page": 587,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5847.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "يَوْمَ لَا تَمْلِكُ نَفْسٌ لِّنَفْسٍ شَيْـًٔا ۖ وَٱلْأَمْرُ يَوْمَئِذٍ لِّلَّهِ",
      "numberInSurah": 19,
      "juz": 30,
      "page": 587,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5848.mp3",
      "translation": "",
      "footnotes": []
    }
  ]
}