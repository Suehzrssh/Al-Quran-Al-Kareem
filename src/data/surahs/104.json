            "juz": 30,
            "page": 601,
            "sajda": false,
            "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6185.mp3",
            "translation": "",
            "footnotes": []
        },
        {
            "text": "ٱلَّتِى تَطَّلِعُ عَلَى ٱلْأَفْـِٔدَةِ",
            "numberInSurah": 7,
            "juz": 30,
            "page": 601,
            "sajda": false,
