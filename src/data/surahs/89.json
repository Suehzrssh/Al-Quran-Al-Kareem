{
  "number": 89,
  "name": "al-Fajr",
  "verses": [
    {
      "text": "وَٱلْفَجْرِ",
      "numberInSurah": 1,
      "juz": 30,
      "page": 593,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5994.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَلَيَالٍ عَشْرٍ",
      "numberInSurah": 2,
      "juz": 30,
      "page": 593,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5995.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَٱلشَّفْعِ وَٱلْوَتْرِ",
      "numberInSurah": 3,
      "juz": 30,
      "page": 593,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5996.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَٱلَّيْلِ إِذَا يَسْرِ",
      "numberInSurah": 4,
      "juz": 30,
      "page": 593,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5997.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "هَلْ فِى ذَٰلِكَ قَسَمٌ لِّذِى حِجْرٍ",
      "numberInSurah": 5,
      "juz": 30,
      "page": 593,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5998.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "أَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِعَادٍ",
      "numberInSurah": 6,
      "juz": 30,
      "page": 593,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5999.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "إِرَمَ ذَاتِ ٱلْعِمَادِ",
      "numberInSurah": 7,
      "juz": 30,
      "page": 593,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6000.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "ٱلَّتِى لَمْ يُخْلَقْ مِثْلُهَا فِى ٱلْبِلَٰدِ",
      "numberInSurah": 8,
      "juz": 30,
      "page": 593,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6001.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَثَمُودَ ٱلَّذِينَ جَابُوا۟ ٱلصَّخْرَ بِٱلْوَادِ",
      "numberInSurah": 9,
      "juz": 30,
      "page": 593,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6002.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَفِرْعَوْنَ ذِى ٱلْأَوْتَادِ",
      "numberInSurah": 10,
      "juz": 30,
      "page": 593,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6003.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "ٱلَّذِينَ طَغَوْا۟ فِى ٱلْبِلَٰدِ",
      "numberInSurah": 11,
      "juz": 30,
      "page": 593,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6004.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "فَأَكْثَرُوا۟ فِيهَا ٱلْفَسَادَ",
      "numberInSurah": 12,
      "juz": 30,
      "page": 593,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6005.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "فَصَبَّ عَلَيْهِمْ رَبُّكَ سَوْطَ عَذَابٍ",
      "numberInSurah": 13,
      "juz": 30,
      "page": 593,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6006.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "إِنَّ رَبَّكَ لَبِٱلْمِرْصَادِ",
      "numberInSurah": 14,
      "juz": 30,
      "page": 593,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6007.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "فَأَمَّا ٱلْإِنسَٰنُ إِذَا مَا ٱبْتَلَىٰهُ رَبُّهُۥ فَأَكرمَهُۥ وَنَعَّمَهُۥ فَيَقُولُ رَبِّىٓ أَكْرَمَنِ",
      "numberInSurah": 15,
      "juz": 30,
      "page": 593,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6008.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَأَمَّآ إِذَا مَا ٱبْتَلَىٰهُ فَقَدَرَ عَلَيْهِ رِزْقَهُۥ فَيَقُولُ رَبِّىٓ أَهَٰنَنِ",
      "numberInSurah": 16,
      "juz": 30,
      "page": 593,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6009.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "كَلَّا ۖ بَل لَّا تُكْرِمُونَ ٱلْيَتِيمَ",
      "numberInSurah": 17,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6010.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَلَا تَحَٰٓضُّونَ عَلَىٰ طَعَامِ ٱلْمِسْكِينِ",
      "numberInSurah": 18,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6011.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَتَأْكُلُونَ ٱلتُّرَاثَ أَكْلًا لَّمًّا",
      "numberInSurah": 19,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6012.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَتُحِبُّونَ ٱلْمَالَ حُبًّا جَمًّا",
      "numberInSurah": 20,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6013.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "كَلَّآ إِذَا دُكَّتِ ٱلْأَرْضُ دَكًّا دَكًّا",
      "numberInSurah": 21,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6014.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَجَآءَ رَبُّكَ وَٱلْمَلَكُ صَفًّا صَفًّا",
      "numberInSurah": 22,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6015.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَجِا۟ىٓءَ يَوْمَئِذٍۢ بِجَهَنَّمَ ۚ يَوْمَئِذٍ يَتَذَكَّرُ ٱلْإِنسَٰنُ وَأَنَّىٰ لَهُ ٱلذِّكْرَىٰ",
      "numberInSurah": 23,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6016.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "يَقُولُ يَٰلَيْتَنِى قَدَّمْتُ لِحَيَاتِى",
      "numberInSurah": 24,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6017.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "فَيَوْمَئِذٍ لَّا يُعَذِّبُ عَذَابَهُۥٓ أَحَدٌ",
      "numberInSurah": 25,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6018.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَلَا يُوثِقُ وَثَاقَهُۥٓ أَحَدٌ",
      "numberInSurah": 26,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6019.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "يَٰٓأَيَّتُهَا ٱلنَّفْسُ ٱلْمُطْمَئِنَّةُ",
      "numberInSurah": 27,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6020.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "ٱرْجِعِىٓ إِلَىٰ رَبِّكِ رَاضِيَةً مَّرْضِيَّةً",
      "numberInSurah": 28,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6021.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "فَٱدْخُلِى فِى عِبَٰدِى",
      "numberInSurah": 29,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6022.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَٱدْخُلِى جَنَّتِى",
      "numberInSurah": 30,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6023.mp3",
      "translation": "",
      "footnotes": []
    }
  ]
}