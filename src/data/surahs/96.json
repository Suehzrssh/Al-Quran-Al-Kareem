      "juz": 30,
      "page": 597,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6107.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "خَلَقَ ٱلْإِنسَٰنَ مِنْ عَلَقٍ",
      "numberInSurah": 2,
      "juz": 30,
      "page": 597,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6108.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "ٱقْرَأْ وَرَبُّكَ ٱلْأَكْرَمُ",
      "numberInSurah": 3,
      "juz": 30,
      "page": 597,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6109.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "ٱلَّذِى عَلَّمَ بِٱلْقَلَمِ",
      "numberInSurah": 4,
      "juz": 30,
      "page": 597,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6110.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "عَلَّمَ ٱلْإِنسَٰنَ مَا لَمْ يَعْلَمْ",
      "numberInSurah": 5,
      "juz": 30,
      "page": 597,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6111.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "كَلَّآ إِنَّ ٱلْإِنسَٰنَ لَيَطْغَىٰٓ",
      "numberInSurah": 6,
      "juz": 30,
      "page": 597,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6112.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "أَن رَّءَاهُ ٱسْتَغْنَىٰٓ",
      "numberInSurah": 7,
      "juz": 30,
      "page": 597,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6113.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "إِنَّ إِلَىٰ رَبِّكَ ٱلرُّجْعَىٰٓ",
      "numberInSurah": 8,
      "juz": 30,
      "page": 597,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6114.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "أَرَءَيْتَ ٱلَّذِى يَنْهَىٰ",
      "numberInSurah": 9,
      "juz": 30,
      "page": 597,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6115.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "عَبْدًا إِذَا صَلَّىٰٓ",
      "numberInSurah": 10,
      "juz": 30,
      "page": 597,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6116.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "أَرَءَيْتَ إِن كَانَ عَلَى ٱلْهُدَىٰٓ",
      "numberInSurah": 11,
      "juz": 30,
      "page": 597,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6117.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "أَوْ أَمَرَ بِٱلتَّقْوَىٰٓ",
      "numberInSurah": 12,
      "juz": 30,
      "page": 597,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6118.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "أَرَءَيْتَ إِن كَذَّبَ وَتَوَلَّىٰٓ",
      "numberInSurah": 13,
      "juz": 30,
      "page": 597,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6119.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "أَلَمْ يَعْلَم بِأَنَّ ٱللَّهَ يَرَىٰ",
      "numberInSurah": 14,
      "juz": 30,
      "page": 598,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6120.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "كَلَّا لَئِن لَّمْ يَنتَهِ لَنَسْفَعًۢا بِٱلنَّاصِيَةِ",
      "numberInSurah": 15,
      "juz": 30,
      "page": 598,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6121.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "نَاصِيَةٍ كَٰذِبَةٍ خَاطِئَةٍ",
      "numberInSurah": 16,
      "juz": 30,
      "page": 598,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6122.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "فَلْيَدْعُ نَادِيَهُۥ",
      "numberInSurah": 17,
      "juz": 30,
      "page": 598,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6123.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "سَنَدْعُ ٱلزَّبَانِيَةَ",
      "numberInSurah": 18,
      "juz": 30,
      "page": 598,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6124.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "كَلَّا لَا تُطِعْهُ وَٱسْجُدْ وَٱقْتَرِب ۩",
      "numberInSurah": 19,
      "juz": 30,
      "page": 598,
      "sajda": true,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6125.mp3",
