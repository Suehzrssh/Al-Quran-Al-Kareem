{
  "number": 88,
  "name": "al-Ghashiyah",
  "verses": [
    {
      "text": "هَلْ أَتَىٰكَ حَدِيثُ ٱلْغَٰشِيَةِ",
      "numberInSurah": 1,
      "juz": 30,
      "page": 592,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5968.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وُجُوهٌ يَوْمَئِذٍ خَٰشِعَةٌ",
      "numberInSurah": 2,
      "juz": 30,
      "page": 592,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5969.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "عَامِلَةٌ نَّاصِبَةٌ",
      "numberInSurah": 3,
      "juz": 30,
      "page": 592,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5970.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "تَصْلَىٰ نَارًا حَامِيَةً",
      "numberInSurah": 4,
      "juz": 30,
      "page": 592,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5971.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "تُسْقَىٰ مِنْ عَيْنٍ ءَانِيَةٍ",
      "numberInSurah": 5,
      "juz": 30,
      "page": 592,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5972.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "لَّيْسَ لَهُمْ طَعَامٌ إِلَّا مِن ضَرِيعٍ",
      "numberInSurah": 6,
      "juz": 30,
      "page": 592,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5973.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "لَّا يُسْمِنُ وَلَا يُغْنِى مِن جُوعٍ",
      "numberInSurah": 7,
      "juz": 30,
      "page": 592,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5974.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وُجُوهٌ يَوْمَئِذٍ نَّاعِمَةٌ",
      "numberInSurah": 8,
      "juz": 30,
      "page": 592,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5975.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "لِّسَعْيِهَا رَاضِيَةٌ",
      "numberInSurah": 9,
      "juz": 30,
      "page": 592,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5976.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "فِى جَنَّةٍ عَالِيَةٍ",
      "numberInSurah": 10,
      "juz": 30,
      "page": 592,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5977.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "لَّا تَسْمَعُ فِيهَا لَٰغِيَةً",
      "numberInSurah": 11,
      "juz": 30,
      "page": 592,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5978.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "فِيهَا عَيْنٌ جَارِيَةٌ",
      "numberInSurah": 12,
      "juz": 30,
      "page": 592,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5979.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "فِيهَا سُرُرٌ مَّرْفُوعَةٌ",
      "numberInSurah": 13,
      "juz": 30,
      "page": 592,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5980.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَأَكْوَابٌ مَّوْضُوعَةٌ",
      "numberInSurah": 14,
      "juz": 30,
      "page": 592,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5981.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَنَمَارِقُ مَصْفُوفَةٌ",
      "numberInSurah": 15,
      "juz": 30,
      "page": 592,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5982.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَزَرَابِىُّ مَبْثُوثَةٌ",
      "numberInSurah": 16,
      "juz": 30,
      "page": 592,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5983.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "أَفَلَا يَنظُرُونَ إِلَى ٱلْإِبِلِ كَيْفَ خُلِقَتْ",
      "numberInSurah": 17,
      "juz": 30,
      "page": 592,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5984.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَإِلَى ٱلسَّمَآءِ كَيْفَ رُفِعَتْ",
      "numberInSurah": 18,
      "juz": 30,
      "page": 592,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5985.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَإِلَى ٱلْجِبَالِ كَيْفَ نُصِبَتْ",
      "numberInSurah": 19,
      "juz": 30,
      "page": 592,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5986.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَإِلَى ٱلْأَرْضِ كَيْفَ سُطِحَتْ",
      "numberInSurah": 10,
      "juz": 30,
      "page": 592,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5987.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "فَذَكِّرْ إِنَّمَآ أَنتَ مُذَكِّرٌ",
      "numberInSurah": 21,
      "juz": 30,
      "page": 592,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5988.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "لَّسْتَ عَلَيْهِم بِمُصَيْطِرٍ",
      "numberInSurah": 22,
      "juz": 30,
      "page": 592,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5989.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "إِلَّا مَن تَوَلَّىٰ وَكَفَرَ",
      "numberInSurah": 23,
      "juz": 30,
      "page": 592,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5990.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "فَيُعَذِّبُهُ ٱللَّهُ ٱلْعَذَابَ ٱلْأَكْبَرَ",
      "numberInSurah": 24,
      "juz": 30,
      "page": 592,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5991.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "إِنَّ إِلَيْنَآ إِيَابَهُمْ",
      "numberInSurah": 25,
      "juz": 30,
      "page": 592,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5992.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "ثُمَّ إِنَّ عَلَيْنَا حِسَابَهُم",
      "numberInSurah": 26,
      "juz": 30,
      "page": 592,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5993.mp3",
      "translation": "",
      "footnotes": []
    }
  ]
}