{
  "number": 90,
  "name": "al-Balad",
  "verses": [
    {
      "text": "لَآ أُقْسِمُ بِهَٰذَا ٱلْبَلَدِ",
      "numberInSurah": 1,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6024.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَأَنتَ حِلٌّۢ بِهَٰذَا ٱلْبَلَدِ",
      "numberInSurah": 2,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6025.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَوَالِدٍ وَمَا وَلَدَ",
      "numberInSurah": 3,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6026.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "لَقَدْ خَلَقْنَا ٱلْإِنسَٰنَ فِى كَبَدٍ",
      "numberInSurah": 4,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6027.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "أَيَحْسَبُ أَن لَّن يَقْدِرَ عَلَيْهِ أَحَدٌ",
      "numberInSurah": 5,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6028.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "يَقُولُ أَهْلَكْتُ مَالًا لُّبَدًا",
      "numberInSurah": 6,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6029.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "أَيَحْسَبُ أَن لَّمْ يَرَهُۥٓ أَحَدٌ",
      "numberInSurah": 7,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6030.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "أَلَمْ نَجْعَل لَّهُۥ عَيْنَيْنِ",
      "numberInSurah": 8,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6031.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَلِسَانًا وَشَفَتَيْنِ",
      "numberInSurah": 9,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6032.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَهَدَيْنَٰهُ ٱلنَّجْدَيْنِ",
      "numberInSurah": 10,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6033.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "فَلَا ٱقْتَحَمَ ٱلْعَقَبَةَ",
      "numberInSurah": 11,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6034.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَمَآ أَدْرَىٰكَ مَا ٱلْعَقَبَةُ",
      "numberInSurah": 12,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6035.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "فَكُّ رَقَبَةٍ",
      "numberInSurah": 13,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6036.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "أَوْ إِطْعَٰمٌ فِى يَوْمٍ ذِى مَسْغَبَةٍ",
      "numberInSurah": 14,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6037.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "يَتِيمًا ذَا مَقْرَبَةٍ",
      "numberInSurah": 15,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6038.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "أَوْ مِسْكِينًا ذَا مَتْرَبَةٍ",
      "numberInSurah": 16,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6039.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "ثُمَّ كَانَ مِنَ ٱلَّذِينَ ءَامَنُوا۟ وَتَوَاصَوْا۟ بِٱلصَّبْرِ وَتَوَاصَوْا۟ بِٱلْمَرْحَمَةِ",
      "numberInSurah": 17,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6040.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "أُو۟لَٰٓئِكَ أَصْحَٰبُ ٱلْمَيْمَنَةِ",
      "numberInSurah": 18,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6041.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَٱلَّذِينَ كَفَرُوا۟ بِـَٔايَٰتِنَا هُمْ أَصْحَٰبُ ٱلْمَشْـَٔمَةِ",
      "numberInSurah": 19,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6042.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "عَلَيْهِمْ نَارٌ مُّؤْصَدَةٌۢ",
      "numberInSurah": 20,
      "juz": 30,
      "page": 594,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6043.mp3",
      "translation": "",
      "footnotes": []
    }
  ]
}