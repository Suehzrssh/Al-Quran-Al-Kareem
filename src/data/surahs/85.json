{
  "number": 85,
  "name": "al-Buruj",
  "verses": [
    {
      "text": "وَٱلسَّمَآءِ ذَاتِ ٱلْبُرُوجِ",
      "numberInSurah": 1,
      "juz": 30,
      "page": 590,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5910.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَٱلْيَوْمِ ٱلْمَوْعُودِ",
      "numberInSurah": 2,
      "juz": 30,
      "page": 590,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5911.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَشَاهِدٍ وَمَشْهُودٍ",
      "numberInSurah": 3,
      "juz": 30,
      "page": 590,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5912.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "قُتِلَ أَصْحَٰبُ ٱلْأُخْدُودِ",
      "numberInSurah": 4,
      "juz": 30,
      "page": 590,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5913.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "ٱلنَّارِ ذَاتِ ٱلْوَقُودِ",
      "numberInSurah": 5,
      "juz": 30,
      "page": 590,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5914.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "إِذْ هُمْ عَلَيْهَا قُعُودٌ",
      "numberInSurah": 6,
      "juz": 30,
      "page": 590,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5915.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَهُمْ عَلَىٰ مَا يَفْعَلُونَ بِٱلْمُؤْمِنِينَ شُهُودٌ",
      "numberInSurah": 7,
      "juz": 30,
      "page": 590,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5916.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَمَا نَقَمُوا۟ مِنْهُمْ إِلَّآ أَن يُؤْمِنُوا۟ بِٱللَّهِ ٱلْعَزِيزِ ٱلْحَمِيدِ",
      "numberInSurah": 8,
      "juz": 30,
      "page": 590,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5917.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "ٱلَّذِى لَهُۥ مُلْكُ ٱلسَّمَٰوَٰتِ وَٱلْأَرْضِ ۚ وَٱللَّهُ عَلَىٰ كُلِّ شَىْءٍ شَهِيدٌ",
      "numberInSurah": 9,
      "juz": 30,
      "page": 590,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5918.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "إِنَّ ٱلَّذِينَ فَتَنُوا۟ ٱلْمُؤْمِنِينَ وَٱلْمُؤْمِنَٰتِ ثُمَّ لَمْ يَتُوبُوا۟ فَلَهُمْ عَذَابُ جَهَنَّمَ وَلَهُمْ عَذَابُ ٱلْحَرِيقِ",
      "numberInSurah": 10,
      "juz": 30,
      "page": 590,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5919.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "إِنَّ ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّٰلِحَٰتِ لَهُمْ جَنَّٰتٌۭ تَجْرِى مِن تَحْتِهَا ٱلْأَنْهَٰرُ ۚ ذَٰلِكَ ٱلْفَوْزُ ٱلْكَبِيرُ",
      "numberInSurah": 11,
      "juz": 30,
      "page": 590,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5920.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "إِنَّ بَطْشَ رَبِّكَ لَشَدِيدٌ",
      "numberInSurah": 12,
      "juz": 30,
      "page": 590,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5921.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "إِنَّهُۥ هُوَ يُبْدِئُ وَيُعِيدُ",
      "numberInSurah": 13,
      "juz": 30,
      "page": 590,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5922.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَهُوَ ٱلْغَفُورُ ٱلْوَدُودُ",
      "numberInSurah": 14,
      "juz": 30,
      "page": 590,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5923.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "ذُو ٱلْعَرْشِ ٱلْمَجِيدُ",
      "numberInSurah": 15,
      "juz": 30,
      "page": 590,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5924.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "فَعَّالٌ لِّمَا يُرِيدُ",
      "numberInSurah": 16,
      "juz": 30,
      "page": 590,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5925.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "هَلْ أَتَىٰكَ حَدِيثُ ٱلْجُنُودِ",
      "numberInSurah": 17,
      "juz": 30,
      "page": 590,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5926.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "فِرْعَوْنَ وَثَمُودَ",
      "numberInSurah": 18,
      "juz": 30,
      "page": 590,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5927.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "بَلِ ٱلَّذِينَ كَفَرُوا۟ فِى تَكْذِيبٍ",
      "numberInSurah": 19,
      "juz": 30,
      "page": 590,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5928.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَٱللَّهُ مِن وَرَآئِهِم مُّحِيطٌ",
      "numberInSurah": 20,
      "juz": 30,
      "page": 590,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5929.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "بَلْ هُوَ قُرْءَانٌ مَّجِيدٌ",
      "numberInSurah": 21,
      "juz": 30,
      "page": 590,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5930.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "فِى لَوْحٍ مَّحْفُوظٍۭ",
      "numberInSurah": 22,
      "juz": 30,
      "page": 590,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5931.mp3",
      "translation": "",
      "footnotes": []
    }
  ]
}