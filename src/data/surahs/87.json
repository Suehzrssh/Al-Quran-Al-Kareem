{
  "number": 87,
  "name": "al-A'la",
  "verses": [
    {
      "text": "سَبِّحِ ٱسْمَ رَبِّكَ ٱلْأَعْلَى",
      "numberInSurah": 1,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5949.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "ٱلَّذِى خَلَقَ فَسَوَّىٰ",
      "numberInSurah": 2,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5950.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَٱلَّذِى قَدَّرَ فَهَدَىٰ",
      "numberInSurah": 3,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5951.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَٱلَّذِىٓ أَخْرَجَ ٱلْمَرْعَىٰ",
      "numberInSurah": 4,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5952.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "فَجَعَلَهُۥ غُثَآءً أَحْوَىٰ",
      "numberInSurah": 5,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5953.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "سَنُقْرِئُكَ فَلَا تَنسَىٰٓ",
      "numberInSurah": 6,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5954.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "إِلَّا مَا شَآءَ ٱللَّهُ ۚ إِنَّهُۥ يَعْلَمُ ٱلْجَهْرَ وَمَا يَخْفَىٰ",
      "numberInSurah": 7,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5955.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَنُيَسِّرُكَ لِلْيُسْرَىٰ",
      "numberInSurah": 8,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5956.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "فَذَكِّرْ إِن نَّفَعَتِ ٱلذِّكْرَىٰ",
      "numberInSurah": 9,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5957.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "سَيَذَّكَّرُ مَن يَخْشَىٰ",
      "numberInSurah": 10,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5958.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَيَتَجَنَّبُهَا ٱلْأَشْقَى",
      "numberInSurah": 11,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5959.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "ٱلَّذِى يَصْلَى ٱلنَّارَ ٱلْكُبْرَىٰ",
      "numberInSurah": 12,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5960.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحْيَىٰ",
      "numberInSurah": 13,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5961.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "قَدْ أَفْلَحَ مَن تَزَكَّىٰ",
      "numberInSurah": 14,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5962.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَذَكَرَ ٱسْمَ رَبِّهِۦ فَصَلَّىٰ",
      "numberInSurah": 15,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5963.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "بَلْ تُؤْثِرُونَ ٱلْحَيَٰوةَ ٱلدُّنْيَا",
      "numberInSurah": 16,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5964.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَٱلْءَاخِرَةُ خَيْرٌ وَأَبْقَىٰٓ",
      "numberInSurah": 17,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5965.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "إِنَّ هَٰذَا لَفِى ٱلصُّحُفِ ٱلْأُولَىٰ",
      "numberInSurah": 18,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5966.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "صُحُفِ إِبْرَٰهِيمَ وَمُوسَىٰ",
      "numberInSurah": 19,
      "juz": 30,
      "page": 591,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/5967.mp3",
      "translation": "",
      "footnotes": []
    }
  ]
}