{
  "number": 91,
  "name": "ash-Shams",
  "verses": [
    {
      "text": "وَٱلشَّمْسِ وَضُحَىٰهَا",
      "numberInSurah": 1,
      "juz": 30,
      "page": 595,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6044.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَٱلْقَمَرِ إِذَا تَلَىٰهَا",
      "numberInSurah": 2,
      "juz": 30,
      "page": 595,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6045.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَٱلنَّهَارِ إِذَا جَلَّىٰهَا",
      "numberInSurah": 3,
      "juz": 30,
      "page": 595,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6046.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَٱلَّيْلِ إِذَا يَغْشَىٰهَا",
      "numberInSurah": 4,
      "juz": 30,
      "page": 595,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6047.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَٱلسَّمَآءِ وَمَا بَنَىٰهَا",
      "numberInSurah": 5,
      "juz": 30,
      "page": 595,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6048.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَٱلْأَرْضِ وَمَا طَحَىٰهَا",
      "numberInSurah": 6,
      "juz": 30,
      "page": 595,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6049.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَنَفْسٍۢ وَمَا سَوَّىٰهَا",
      "numberInSurah": 7,
      "juz": 30,
      "page": 595,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6050.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "فَأَلْهَمَهَا فُجُورَهَا وَتَقْوَىٰهَا",
      "numberInSurah": 8,
      "juz": 30,
      "page": 595,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6051.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "قَدْ أَفْلَحَ مَن زَكَّىٰهَا",
      "numberInSurah": 9,
      "juz": 30,
      "page": 595,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6052.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَقَدْ خَابَ مَن دَسَّىٰهَا",
      "numberInSurah": 10,
      "juz": 30,
      "page": 595,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6053.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "كَذَّبَتْ ثَمُودُ بِطَغْوَىٰهَآ",
      "numberInSurah": 11,
      "juz": 30,
      "page": 595,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6054.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "إِذِ ٱنۢبَعَثَ أَشْقَىٰهَا",
      "numberInSurah": 12,
      "juz": 30,
      "page": 595,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6055.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "فَقَالَ لَهُمْ رَسُولُ ٱللَّهِ نَاقَةَ ٱللَّهِ وَسُقْيَٰهَا",
      "numberInSurah": 13,
      "juz": 30,
      "page": 595,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6056.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "فَكَذَّبُوهُ فَعَقَرُوهَا فَدَمْدَمَ عَلَيْهِمْ رَبُّهُم بِذَنۢبِهِمْ فَسَوَّىٰهَا",
      "numberInSurah": 14,
      "juz": 30,
      "page": 595,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6057.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَلَا يَخَافُ عُقْبَٰهَا",
      "numberInSurah": 15,
      "juz": 30,
      "page": 595,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6058.mp3",
      "translation": "",
      "footnotes": []
    }
  ]
}