  "number": 100,
  "name": "al-Adiyat",
  "verses": [
    {
      "text": "وَٱلْعَٰدِيَٰتِ ضَبْحًا",
      "numberInSurah": 1,
      "juz": 30,
      "page": 599,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6147.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "فَٱلْمُورِيَٰتِ قَدْحًا",
      "numberInSurah": 2,
      "juz": 30,
      "page": 599,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6148.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "فَٱلْمُغِيرَٰتِ صُبْحًا",
      "numberInSurah": 3,
      "juz": 30,
      "page": 599,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6149.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "فَأَثَرْنَ بِهِۦ نَقْعًا",
      "numberInSurah": 4,
      "juz": 30,
      "page": 599,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6150.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "فَوَسَطْنَ بِهِۦ جَمْعًا",
      "numberInSurah": 5,
      "juz": 30,
      "page": 599,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6151.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "إِنَّ ٱلْإِنسَٰنَ لِرَبِّهِۦ لَكَنُودٌ",
      "numberInSurah": 6,
      "juz": 30,
      "page": 599,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6152.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَإِنَّهُۥ عَلَىٰ ذَٰلِكَ لَشَهِيدٌ",
      "numberInSurah": 7,
      "juz": 30,
      "page": 599,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6153.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَإِنَّهُۥ لِحُبِّ ٱلْخَيْرِ لَشَدِيدٌ",
      "numberInSurah": 8,
      "juz": 30,
      "page": 599,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6154.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "أَفَلَا يَعْلَمُ إِذَا بُعْثِرَ مَا فِى ٱلْقُبُورِ",
      "numberInSurah": 9,
      "juz": 30,
      "page": 599,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6155.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "وَحُصِّلَ مَا فِى ٱلصُّدُورِ",
      "numberInSurah": 10,
      "juz": 30,
      "page": 599,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6156.mp3",
      "translation": "",
      "footnotes": []
    },
    {
      "text": "إِنَّ رَبَّهُم بِهِمْ يَوْمَئِذٍۢ لَّخَبِيرٌۢ",
      "numberInSurah": 11,
      "juz": 30,
      "page": 599,
      "sajda": false,
      "audio": "https://cdn.islamic.network/quran/audio/64/ar.saoodshuraym/6157.mp3",
      "translation": "",
      "footnotes": []
    }
  ]
}